{
    "name": "jaybizzle/laravel-zencoder",
    "description": "A laravel wrapper around the zencoder API.",
    "keywords": ["a4m", "api", "zencoder", "laravel", "transcoding", "video", "audio", "thumbnails"],
    "homepage": "https://gitlab.advantage4me.co/opensource/zencoder/",
    "license": "MIT",
    "support": {
        "email": "mbeech@mark-beech.co.uk",
        "issues": "https://gitlab.advantage4me.co/opensource/zencoder/issues",
        "source": "https://gitlab.advantage4me.co/opensource/zencoder"
    },
    "authors": [
        {
            "name": "Mark Beech",
            "email": "mbeech@mark-beech.co.uk",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=5.4",
        "zencoder/zencoder-php": "2.2.*",
        "illuminate/support": "7.*",
        "rmccue/requests": ">=1.0",
        "nesbot/carbon": "2.*"
    },
    "require-dev": {
        "mockery/mockery": "dev-master@dev"
    },
    "autoload": {
        "psr-4": {
            "Jaybizzle\\Zencoder\\": "src",
            "Jaybizzle\\Zencoder\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Jaybizzle\\Zencoder\\ZencoderServiceProvider"
            ],
            "aliases": {
                "Zencoder": "Jaybizzle\\Zencoder\\Facades"
            }
        }
    }
}
